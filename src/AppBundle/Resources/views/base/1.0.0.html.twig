<!doctype>
<html>
	<head>
		<title>CSCFA Jenkins build publisher</title>
		
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
		{% stylesheets '@AppBundle/Resources/public/css/*.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        
    	{% block style %}
    	{% endblock %}
    	{% block javascript %}
    	{% endblock %}
	</head>
	<body class='container'>
		<header id='site_header'>
			<div class='row'>
    			<div id='site_logo_container' class='col-xs-2'>
        			{% image '@AppBundle/Resources/public/images/CSCFA-logo.png' %}
        			<img alt="cscfa-logo" src="{{ asset_url }}" id='site_logo'/>
        			{% endimage %}
    			</div>
    			<h2 id='site_title' class='col-xs-10'>
    				Jenkins build publisher
    			</h2>
			</div>
		</header>
	{% block breadcrumb %}
	{% endblock %}
	
	{% block content %}
	{% endblock %}
	</body>
</html>
